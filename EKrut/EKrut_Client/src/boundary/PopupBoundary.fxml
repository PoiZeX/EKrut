                  <Insets />
               </opaqueInsets>
               <font>
                  <Font name="Calibri Bold" size="41.0" />
               </font>
               <GridPane.margin>
                  <Insets left="10.0" />
               </GridPane.margin>
            </Label>
            <ImageView fx:id="iconImageView" fitHeight="54.0" fitWidth="65.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="1" GridPane.halignment="CENTER" />
